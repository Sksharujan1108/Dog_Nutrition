{
  "cli": {
    "version": ">= 3.10.0",
    "requireCommit": true,
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "production": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "android": {
        "buildType": "apk",
        "developmentClient": true
      },
      "ios": {
        "simulator": true,
        "developmentClient": true
      }
    }
  }
}
